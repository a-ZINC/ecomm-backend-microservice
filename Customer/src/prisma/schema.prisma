datasource db {
    url      = env("DATABASE_URL")
    provider = "postgresql"
}
  
generator client {
    provider = "prisma-client-js"
}

model Customer {
    id        String      @id @default(uuid())
    createdAt DateTime @default(now())
    email     String   @unique
    name      String?
    password  String?
    address   String?
    productId  String[]
    wishlist String[]
}

